<html>

<head>
    <title>InFeed</title>
    <link rel="stylesheet" href="styles/navbar.css">
    <link rel="stylesheet" href="styles/signup.css">
    <link rel="icon" href="images/web-icon.png">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,700;1,300&display=swap" rel="stylesheet">

    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <!--<script src="scripts/user-reg.js"></script>-->
    <script type="text/javascript" src="scripts/index.js"></script>
</head>
<body>
        
    {{nav}}
    <!-- Where the rest of the page is: -->
    <div id="main-page">
        <div style="height:50px"></div>
        <div class="main-wrapper">
            
            <div id="log">
                <div id="log-header">

                    <img src="images/web-icon.png">
                    <span id="headline">Welcome to InFeed!</span>
                </div>

                <form>
                    <div id="log-form">
                        
                        <input type="text" placeholder="Username" id="form-user-log">
                        <input type="password" placeholder="Password" id="form-pass-log">

                        <button id="login">Log In</button>
                        <a href="http://localhost:3000/user_reg" id="newsignup">Sign up</a>
                    </div>
                </form>
                
            </div>
        </div>
    </div>
</body>
</html>